zx_deadline_after(0xf0000)
zx_nanosleep(0xf0000)
zx_ticks_get()

# TODO: According to comments in misc.fidl, this should never return an error.

zx_ticks_per_second() # ZX_ERR_SHOULD_WAIT

r0 = syz_vmar_root_self()
zx_vmar_allocate(r0, 0x40, 0x0, 0x1000, &AUTO=<r1=>0x0, &AUTO=<r2=>0x0)
r3 = syz_process_self()
zx_thread_create(r3, &AUTO='test_thread', 0xb, 0x0, &AUTO=<r4=>0x0)

# Does not return if successful; should we disable it?

zx_vmar_unmap_handle_close_thread_exit(r1, r2, 0x1000, r4)